                            }
                        });
                    }).catch(error => {
                        console.error('Error:', error);
                    });

                    // Tembak Api check token untuk memastikan token masih aktif atau tidak
                    var url = 'http://localhost:8080/api/v1/dashboard/profile-check';
                    fetch(url, {
                        method: 'GET',
                        headers: {
                            'Authorization': 'Bearer ' + token
