                                    duration: 5000,
                                    newWindow: true,
                                    close: true,
                                    gravity: "top",
                                    position: "right",
                                    stopOnFocus: true,
                                }).showToast();
                            }
                        })
                    })

                    // Panggil data master pajak layanan
                    var url = 'http://localhost:8080/api/v1/pajak-layanan/master-data';
                    fetch(url, {
                        method: 'GET',
                        headers: {
